{
  "name": "functions",
  "version": "0.0.1",
  "description": "---",
  "author": "Cave Lab",
  "scripts": {
    "build-full": "npm run lint && npm run format && npm run build",
    "lint": "eslint --ext .js,.ts . --fix",
    "format": "prettier --write .",
    "build": "tsc",
    "watch": "tsc --watch --preserveWatchOutput",
    "serve": "concurrently \"npm run watch\" \"firebase emulators:start --only auth,database,firestore,functions\" --import=../emulator_data --export-on-exit",
    "deploy": "firebase deploy --only \"functions,firestore,database\"",
    "deploy-single": "firebase deploy --only functions:api"
  },
  "dependencies": {
    "axios": "^1.6.8",
    "express": "^4.19.2",
    "firebase-admin": "^12.1.0",
    "firebase-functions": "^5.0.1",
    "lodash": "^4.17.21",
    "luxon": "^3.4.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/lodash": "^4.17.4",
    "@types/luxon": "^3.4.2",
    "@typescript-eslint/eslint-plugin": "^7.9.0",
    "@typescript-eslint/parser": "^7.9.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.5",
    "typescript": "^5.4.5"
  },
  "main": "lib/index.js",
  "engines": {
    "node": "18"
  },
  "private": true
}
